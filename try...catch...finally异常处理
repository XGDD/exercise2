public class Test{
    public static void main(String[] args){
        System.out.println("1.计算开始前");
        try{
            System.out.println("2.进行算术运算："+10/2);
        }catch(ArithmeticException e){
            e.printStackTrace();
        }finally{
            System.out.println("3.不管是否产生异常，都会执行此程序");
        }
        System.out.println("4.计算结束后");
    }
}
