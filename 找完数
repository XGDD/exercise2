public class Test{
    public static void main(String[] args){
        for(int i = 1; i <= 1000; i++)
        {
            Fun(i);
        }
    }
    public static void Fun(int i){
        int ret = 0;
        for(int j = 1; j < i; j++){
            if(i % j == 0){
                ret += j;
            }
        }
        if(ret == i)
            System.out.print(ret+" ");
    }
}
