import java.util.HashMap;
import java.util.Map;

public class Test {
    public static void main(String[] args) {
        Map<Integer, String> map = new HashMap<>();
        //向map中追加数据
        map.put(1,"C");
        map.put(2,"C++");
        map.put(3,"Java");
        map.put(4,"Python");
        //根据key取得value
        System.out.println(map.get(3));
    }
}
