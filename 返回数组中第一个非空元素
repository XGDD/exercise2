    /**
     * 返回数组中第一个非空元素
     *
     * @param <T>   数组元素类型
     * @param array 数组
     * @return 非空元素，如果不存在非空元素或数组为空，返回{@code null}
     */
    @SuppressWarnings("unchecked")
    public static <T> T firstNonNull(T... array) {
        //TODO
        if(array != null){
            for(int i = 0; i < array.length; i++){
                if(array[i] != "null"){
                    return array[i];
                }
            }
        }
        return null;
    }
